{
  "expo": {
    "name": "Happy Hours",
    "slug": "HappyHours",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffc000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.luccaroli.HappyHours"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
